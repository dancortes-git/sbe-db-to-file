{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Debug (Launch) - Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Debug (Launch)-SpringBatchExamplesApplication<SpringBatchExamples>",
      "request": "launch",
      "mainClass": "br.com.dcc.springbatchexamples.SpringBatchExamplesApplication",
      "projectName": "SpringBatchExamples",
      "args": ["parameter=7"],
      "env": {
        "REPOSITORY_DRIVER": "org.postgresql.Driver",
        "REPOSITORY_URL": "jdbc:postgresql://localhost/springbatch",
        "REPOSITORY_USER": "springbatch",
        "REPOSITORY_PASS": "springbatch",
      }
    }
  ]
}